{
    "name": "beastbytes/yii-otp",
    "description": "Library that simplifies using One Time Passwords (OTP) (HOTP or TOTP algorithm) in Yii3 applications",
    "keywords": [
        "HMAC-Based one-time password",
        "hotp",
        "totp",
        "time-based one-time password",
        "one time password",
        "RFC 4226",
        "RFC 6238",
        "tfa",
        "2fa",
        "two-factor-authentication",
        "yii3"
    ],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "chris",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "chillerlan/php-qrcode": "^5.0",
        "httpsoft/http-message": "^1.0",
        "spomky-labs/otphp": "^11.0",
        "symfony/clock": "^7.0",
        "yiisoft/cookies": "^1.0",
        "yiisoft/definitions": "^3.0",
        "yiisoft/db": "dev-master",
        "yiisoft/form": "^1.0",
        "yiisoft/form-model": "^1.0",
        "yiisoft/security": "^1.0",
        "yiisoft/translator": "^3.0",
        "yiisoft/user": "^2.0",
        "yiisoft/view": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^6.0",
        "yiisoft/db-migration": "dev-master",
        "yiisoft/db-sqlite": "dev-master",
        "yiisoft/cache": "^3.0",
        "yiisoft/injector": "^1.0",
        "yiisoft/test-support": "^3.0",
        "yiisoft/translator-extractor": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Yii\\Otp\\": "src/",
            "BeastBytes\\Yii\\Otp\\Migration\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\Yii\\Otp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "di": "di.php",
            "params": "params.php"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true,
            "yiisoft/config": true
        }
    }
}